{
  "name": "mac-windows",
  "version": "0.5.0",
  "description": "Provide information about Application Windows running and activate and Application.",
  "main": "index.js",
  "scripts": {
    "build-activate-window": "swift build -c release -Xswiftc -static-stdlib --package-path swift/ActivateWindow",
    "build-mac-windows": "swift build -c release -Xswiftc -static-stdlib --package-path swift/MacWindows",
    "start": "node index.js",
    "build": "npm run build-activate-window && npm run build-mac-windows"
  },
  "os": [
    "darwin",
    "!win32",
    "!linux"
  ],
  "keywords": [
    "Mac",
    "Windows",
    "Applications",
    "Dimensions",
    "Position",
    "Activate"
  ],
  "author": "George Karagkiaouris",
  "homepage": "https://github.com/karaggeorge/mac-windows",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/karaggeorge/mac-windows.git"
  },
  "bugs": {
    "url": "https://github.com/karaggeorge/mac-windows/issues"
  }
}
